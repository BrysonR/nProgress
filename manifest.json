{
  "manifest_version": 2,

  "name": "nProgress",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "nProgress_icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "desktopCapture",
    "http://localhost:3000/*",
    "tabs"
  ]
}
